<% provide(:title, t('.edit_field', field_set: @field.field_set.name, field: @field.name)) %>

<%= bootstrap_form_for(
      @field,
      :as => :field,
      :url => { :action => :update }
      ) do |f| %>
  <%= render("common_form_fields", :f => f) %>
<% end %>

<%= render_catalog_admin_fields_modals(@field) %>
